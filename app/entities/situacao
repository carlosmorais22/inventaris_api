class Situacao():

    def __init__(self, descricao, id=None):
        self.__id = id
        self.__descricao = descricao

    @property
    def id(self):
        return self.__id

    @id.setter
    def id(self, id):
        self.__id = id

    @property
    def descricao(self):
        return self.__descricao

    @descricao.setter
    def cpf(self, descricao):
        self.__descricao = descricao
